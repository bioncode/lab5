<%- include('partials/header.ejs') %>
    <body>
        
        <form class="mt-5" action="/search">
            <input class="search-input" type="text" placeholder="Type keyword" name="keyword"/>
            <br><br>
            <button class="search-button brn btn-primary">Search</button>
            <a id="favLink" href="/getKeywords"> See Favorites </a>
        </form>
        <hr>
        <div id="resultsContainer">
            <% imageUrlArray.forEach(function(imageUrl, i) { %>
                <%- (i % 3 == 0)?'<br>':'' %>
                    <img class="image" src='<%= imageUrl %>' width="200" height="200"/>
                    <img class="favoriteIcon" src='img/favorite.png' width="20">
            <% });%>    
        </div>

<%- include('partials/footer.ejs') %>

    </body>
</html>